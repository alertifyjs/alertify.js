{
  "name": "@labeg/alertify.js",
  "description": "An lightweight, unobtrusive customizable JavaScript notification system.",
  "version": "2.0.46",
  "homepage": "https://labeg.github.io/alertify.js/",
  "main": "dist/js/alertify.js",
  "module": "dist/js/alertify.mjs",
  "jsnext:main": "dist/js/alertify.mjs",
  "typings": "dist/js/alertify.d.ts",
  "author": "Fabien Doiron <fabien.doiron@gmail.com> (http://fabien-d.github.io/)",
  "keywords": [
    "alertify",
    "alertifyjs",
    "alertify.js",
    "notifications",
    "alert"
  ],
  "scripts": {
    "packages:manupdate": "rm -rf node_modules && npm install",
    "packages:reinstall": "rm -rf npm-shrinkwrap.json && rm -rf node_modules && npm install && npm dedupe && npm prune && npm shrinkwrap",
    "packages:fullupdate": "ncu -u && npm run packages:reinstall",
    "cs:eslint": "eslint --fix -c .eslintrc.js --ext .tsx,.ts,.jsx,.js ./src/",
    "cs:stylelint": "stylelint --fix ./src/**/*.scss",
    "cs": "concurrently \"npm run cs:eslint\" \"npm run cs:stylelint\"",
    "test": "karma start karma.conf.js",
    "build": "rm -rf dist/ && tsc && rollup -c"
  },
  "repository": "git@github.com:LabEG/alertify.js.git",
  "dependencies": {},
  "devDependencies": {
    "@labeg/code-style": "^1.0.88",
    "@rollup/plugin-commonjs": "^17.0.0",
    "@rollup/plugin-node-resolve": "^11.1.0",
    "autoprefixer": "^10.2.3",
    "concurrently": "^5.3.0",
    "karma": "^6.0.1",
    "karma-chrome-launcher": "^3.1.0",
    "karma-commonjs": "^1.0.0",
    "karma-coverage": "^2.0.3",
    "karma-coveralls": "^2.1.0",
    "karma-firefox-launcher": "^2.1.0",
    "karma-jasmine": "^4.0.1",
    "karma-sauce-launcher": "^4.3.5",
    "node-sass": "^5.0.0",
    "postcss": "^8.2.4",
    "rollup": "^2.38.0",
    "rollup-plugin-postcss": "^4.0.0",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-typescript2": "^0.29.0",
    "typescript": "^4.1.3"
  },
  "license": "MIT"
}
